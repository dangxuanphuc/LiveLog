<div class="table-responsive">
  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th>Live</th>
        <th>#</th>
        <th>Song</th>
        <th>Artist</th>
        <th>Members</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% songs.each do |song| %>
        <% cache_unless logged_in?, song do %>
          <%= render song %>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
