<tr>
  <% if show_live %>
    <td><%= song.live.title %></td>
  <% end %>
  <td>
    <%= song.time_str unless song.time.blank? %>
    <%= song.order %>
  </td>
  <td><%= logged_in? ? link_to(song.name, song) : song.name %></td>
  <td><%= song.artist %></td>
  <td>
    <ul class="list-inline">
      <%= render song.playings %>
    </ul>
  </td>
  <td><%= render "songs/link_to_edit", song: song %></td>
</tr>
